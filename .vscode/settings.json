{
    "editor.rulers": [85],
    "rust-analyzer.cargo.features": [
        "hashlink",
        "mini-moka",
        "quick_cache",
        "stretto",
        "metrics"
    ],
    "rust-analyzer.server.extraEnv": {
        "CARGO_TARGET_DIR": "target/ra"
    },
    "cSpell.words": [
        "clippy",
        "Dharmendra",
        "dogstatsd",
        "hashbrown",
        "Hasher",
        "hashlink",
        "itertools",
        "Modha",
        "moka",
        "mokabench",
        "oltp",
        "pointee",
        "Statsd",
        "thiserror",
        "Toolchain",
        "unsync"
    ],
    "files.watcherExclude": {
        "**/target": true
    }
}
